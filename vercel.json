{
  "version": 2,
  "builds": [
    {
      "src": "backend/deploy_server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/deploy_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/deploy_server.py"
    }
  ],
  "functions": {
    "backend/deploy_server.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "SUPABASE_URL": "https://demo-project.supabase.co",
    "SUPABASE_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.demo-key-for-testing",
    "CORS_ORIGINS": "*",
    "HIPAA_ENCRYPTION_KEY": "demo-encryption-key-change-in-production",
    "ENVIRONMENT": "production",
    "ALLOWED_HOSTS": "*"
  }
}