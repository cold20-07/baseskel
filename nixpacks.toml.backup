[variables]
PYTHON_VERSION = "3.11"

[phases.setup]
nixPkgs = ["python311Full", "python311Packages.pip", "python311Packages.setuptools", "python311Packages.wheel", "python311Packages.virtualenv"]

[phases.install]
cmds = ["bash .nixpacks/setup.sh"]

[start]
cmd = "source /opt/venv/bin/activate && python backend/run_server.py"